<Window x:Class="MessengerBotManager.CheckMDB"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MessengerBotManager"
        mc:Ignorable="d"
        Title="설정 로드중..." Height="150" Width="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50*"/>
            <RowDefinition Height="50*"/>
        </Grid.RowDefinitions>
        <Label x:Name="label" Content="설정 로드중..." VerticalAlignment="Bottom" HorizontalAlignment="Center"/>
        <ProgressBar x:Name="progress" Grid.Row="1" VerticalAlignment="Top" Height="20" Margin="20 0 20 0"/>
    </Grid>
</Window>
